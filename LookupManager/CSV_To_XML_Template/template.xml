<?xml version="1.0" encoding="utf-8" ?>
<!-- template.xml -->
<sh BTS-is-new="1"
    id="{{ id }}"
    name="{{ name }}"
    is-person="{{ is_person }}"
    is-organization="{{ is_organization }}"
    trust="0"
    stakeholder-type="39"
    is-insured="1"
    co-insured="0"
    private-or-org="1"
    sh-judgement="0"
    sh-sequestrated-liquidated="0"
    under_admin="0"
    broker-fee-consent="1"
    include-umbrella-liability="1"
    require-parms="0"
    BTS-category-id="194">
	<person id="{{ person_id }}"
            name="{{ name }}"
            full-names="{{ full_names }}"
            surname="{{ surname }}"
            initials="{{ initials }}"
            identification-no="{{ identification_no }}"
            title-cd="{{ title_cd }}"
            gender-cd="{{ gender_cd }}"
            birth-date="{{ birth_date }}"
            marital-status-cd="{{ marital_status_cd }}"
            id-type="{{ id_type }}"
            passport-number="{{ passport_number }}" />
	<address id="{{ address_id }}"
             ver="1"
             type-cd="PHY"
             line-1="{{ line_1 }}"
             code="{{ code }}"
             suburb="{{ suburb }}"
             residential-area-type="{{ residential_area_type }}"
             years-at-address="{{ years_at_address }}"
             months-at-address="{{ months_at_address }}" />
	<short-term ver="1"
                license-endorsed="{{ license_endorsed }}"
                driver-restriction="{{ driver_restriction }}"
                held-insurance-last-39-days="{{ held_insurance_last_39_days }}"
                claim-count-last-3-years-2="{{ claim_count_last_3_years_2 }}"
                insurer-cancelled="{{ insurer_cancelled }}"
                period-comp-car-insurance="{{ period_comp_car_insurance }}"
                proof-of-ncb="1"
                add-personal-liability="0"
                has-consent="{{ has_consent }}"
                itc-check-done="0"
                policy-excess="4"
                current-premium="0.00"
                held-nonmotor-insurance-last-39-days="0"
                sau-policy-issued="0">
		<license-detail ver="1"
                        seq="1"
                        license-date="{{ license_date }}"
                        license-category="{{ license_category }}"
                        license-type="{{ license_type }}"
                        vehicle-restriction="{{ vehicle_restriction }}" />
	</short-term>
</sh>
